    <variable name="LOGMETHOD" expression="resolveLogMethod(LOGGER, &quot;trace&quot;)" defaultValue="trace(" alwaysStopAt="false" />
    <variable name="THROWABLE" expression="resolveOptionalVariableOfType(&quot;java.lang.Throwable&quot;)" defaultValue="e" alwaysStopAt="false" />
    <variable name="LOGID" expression="createNextLogId(&quot;trace&quot;)" defaultValue="" alwaysStopAt="false" />
    <context>
      <option name="JAVA_CODE" value="true" />
      <option name="JAVA_STATEMENT" value="true" />
      <option name="JAVA_EXPRESSION" value="true" />
      <option name="JAVA_DECLARATION" value="true" />
